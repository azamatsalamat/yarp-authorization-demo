{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "ValidateIssuerSigningKey": true,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidIssuer": "my_issuer",
    "ValidAudience": "my_audience",
    "RequireExpirationTime": true,
    "ValidateLifetime": true,
    "TokenExpires": 86400,
    "AccessTokenSecret": "secret"
  }
}
